{
  "name": "seneca-flow",
  "version": "1.0.4",
  "description": "Senca action flow control, templating and data transformation",
  "main": "flow.js",
  "scripts": {
    "test": "lab -P test -r console -L -t 80 -v",
    "test_parallel": "lab -p -P test -r console -L -t 80 -v",
    "coveralls": "lab -s -P test -r lcov | coveralls",
    "coverage": "lab -v -P test -L -t 80 -r html > docs/coverage.html"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/georgigriffiths/seneca-flow.git"
  },
  "keywords": [
    "seneca",
    "flow control"
  ],
  "author": "Georgi Griffiths (http://georgigriffiths.com/)",
  "license": "MIT",
  "files": [
    "LICENSE",
    "README.md",
    "flow.js"
  ],
  "engines": {
    "node": ">=4.0.0"
  },
  "dependencies": {
    "bson": "^1.0.1",
    "colors": "^1.1.2",
    "lodash": "4.16.4",
    "lru-cache": "^4.0.2",
    "norma": "0.3.0",
    "seneca-parambulator": "^0.3.0",
    "zig": "git://github.com/georgigriffiths/zig.git"
  },
  "devDependencies": {
    "code": "4.0.x",
    "coveralls": "^2.11.14",
    "eslint-config-seneca": "3.x.x",
    "eslint-plugin-standard": "^2.0.1",
    "lab": "11.1.x",
    "seneca": "3.2.x"
  }
}
